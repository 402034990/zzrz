<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.mortgage.vehicle.BpMortgageCarPlaque"
        table="bp_mortgage_car_plaque"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
         <property
            name="steeringWheel"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="steeringWheel"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="leftFrontDoorPlate"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="leftFrontDoorPlate"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="driverSeat"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="driverSeat"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="viceDriverSeat"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="viceDriverSeat"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="rightFrontPlaque"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="rightFrontPlaque"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="leftFrontPlaque"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="leftFrontPlaque"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="afterChair"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="afterChair"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="rightAfterPlaque"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="rightAfterPlaque"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="mortgageid"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="mortgageid"
            length="10"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
