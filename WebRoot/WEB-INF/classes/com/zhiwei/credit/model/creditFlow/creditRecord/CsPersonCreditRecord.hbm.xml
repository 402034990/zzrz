<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.creditRecord.CsPersonCreditRecord"
        table="cs_person_credit_record"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="projectId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="projectId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="personId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="personId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="nameKey"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="nameKey"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="nameValue"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="nameValue"
            length="65535"
            not-null="false"
            unique="false"
        />
        <property
            name="type"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="type"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="interfaceSign"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="interfaceSign"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="reqDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="reqDate"
            length="19"
            unique="false"
        />
        </class>
</hibernate-mapping>
