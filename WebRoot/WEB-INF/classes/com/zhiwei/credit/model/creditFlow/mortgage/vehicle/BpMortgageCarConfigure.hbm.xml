<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.mortgage.vehicle.BpMortgageCarConfigure"
        table="bp_mortgage_car_configure"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
         <property
            name="shift"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="shift"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="airConditioning"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="airConditioning"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="fuel"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="fuel"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="electricDoors"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="electricDoors"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="electricMirror"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="electricMirror"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="aluminiumAlloy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="aluminiumAlloy"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="remoteDoorLock"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remoteDoorLock"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="powerTurn"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="powerTurn"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="addConfiguration"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="addConfiguration"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="mortgageid"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="mortgageid"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="displacement"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="displacement"
            length="50"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
