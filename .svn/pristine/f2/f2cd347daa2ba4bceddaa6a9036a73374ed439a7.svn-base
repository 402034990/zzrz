<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="company" extends="struts-default" strict-method-invocation="false">
		<!-- 添加信息 -->
		<action name="addCompany" class="companyMAction" method="addCompany">
			<result>${successResultValue}</result>
		</action>
		
		<!-- 查看信息从视图 -->
		<action name="seeCompany" class="companyMAction" method="seeCompany"></action>
		
		<!-- 查看信息从责任公司表查询显示在更新页面 -->
		<action name="seeCompanyForUpdate" class="companyMAction" method="seeCompanyForUpdate"></action>
		
		<!-- 更新信息 -->
		<action name="updateCompany" class="companyMAction" method="updateCompany" ></action>
		
	</package>
</struts>