<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
	name="com.bankIntermediary.model.BusinessPlan"
	table="business_plan" select-before-update="false"
	dynamic-update="false" dynamic-insert="false" optimistic-lock="version">
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        
    	<property
	        name="projectId"
	        type="java.lang.Long"
	        update="true"
	        insert="true"
	        access="property"
	        column="projectId"
	        length="20"
	        not-null="true"
	        unique="false"
	      />
    	 <property
	         name="productId"
	         type="java.lang.Long"
	         update="true"
	         insert="true"
	         access="property"
	         column="productId"
	         length="20"
	         not-null="true"
	         unique="false"
	       />
    
    	 <property
	         name="institutionalId"
	         type="java.lang.Long"
	         update="true"
	         insert="true"
	         access="property"
	         column="institutionalId"
	         length="20"
	         not-null="true"
	         unique="false"
	     />
        <property
            name="institutionalAccountManager"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="institutionalAccountManager"
            length="20"
            not-null="true"
            unique="false"
        />
    	<property
	        name="state"
	        type="java.lang.Short"
	        update="true"
	        insert="true"
	        access="property"
	        column="state"
	        length="8"
	        unique="false"
	    />
    	<property
	        name="remark"
	        type="java.lang.String"
	        update="true"
	        insert="true"
	        access="property"
	        column="remark"
	        length="512"
	        not-null="true"
	        unique="false"
	    />
    	<property
	        name="createtime"
	        type="java.util.Date"
	        update="true"
	        insert="true"
	        access="property"
	        column="createtime"
	        length="20"
	        not-null="false"
	        unique="false"
	    />
    	<property
	        name="loanDate"
	        type="java.util.Date"
	        update="true"
	        insert="true"
	        access="property"
	        column="loanDate"
	        length="20"
	        not-null="false"
	        unique="false"
	    />
    	<property
	        name="approvalDate"
	        type="java.util.Date"
	        update="true"
	        insert="true"
	        access="property"
	        column="approvalDate"
	        length="20"
	        not-null="false"
	        unique="false"
	    />
    	<property
	        name="incomeDate"
	        type="java.util.Date"
	        update="true"
	        insert="true"
	        access="property"
	        column="incomeDate"
	        length="20"
	        not-null="false"
	        unique="false"
	    />
	   	 <property
		     name="loanMoney"
		     type="java.math.BigDecimal"
		     update="true"
		     insert="true"
		     access="property"
		     column="loanMoney"
		     precision="20"
		     scale="2"
		     unique="false"
		 />
    	 <property
	         name="projectMoney"
	         type="java.math.BigDecimal"
	         update="true"
	         insert="true"
	         access="property"
	         column="projectMoney"
	         precision="20"
	         scale="2"
	         unique="false"
	     />
    </class>
</hibernate-mapping>