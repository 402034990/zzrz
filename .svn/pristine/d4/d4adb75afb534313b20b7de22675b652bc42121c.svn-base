<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.mortage.CsProcreditMortgageRecord"
        table="cs_procredit_mortgage_record"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="mortgageId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="mortgageId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="storehouseDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="storehouseDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="storehouseStatus"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="storehouseStatus"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="storehousePersonId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="storehousePersonId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="storehouseSite"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="storehouseSite"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="storehouseRemark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="storehouseRemark"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="operateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="operateDate"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
