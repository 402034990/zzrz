<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.customer.enterprise.BpDicAccountTitle"
        table="bp_dic_account_title"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="keyName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="keyName"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="titleClass"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="titleClass"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="title"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="title"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remark"
            length="255"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
