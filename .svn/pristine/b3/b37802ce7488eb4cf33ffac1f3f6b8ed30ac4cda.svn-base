	<systemConfig>
		<systemVersion>Version 3.5</systemVersion>
		<isGroupVersion>false</isGroupVersion>
		<isOA>true</isOA>
		<!-- 宽限日  -->
		<graceDayNum>5</graceDayNum>
		<!-- Ftp配置 -->
<!--		<ftpIp>192.168.1.103</ftpIp>-->
<!--		<ftpUsName>zyht</ftpUsName>-->
